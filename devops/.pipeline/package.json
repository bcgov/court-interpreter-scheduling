{
  "name": "court-scheduler-pipeline",
  "version": "1.0.1",
  "description": "Pipeline build script for CourtScheduler app",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "oc-build": "tsc && DEBUG=*  node ./dist/app.js --action=build",
    "oc-deploy": "tsc && DEBUG=*  node ./dist/app.js --action=deploy",
    "oc-delete": "tsc && DEBUG=*  node ./dist/app.js --action=delete"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreshworksStudio/Court-scheduler.git"
  },
  "keywords": [
    "pipeline",
    "openshift"
  ],
  "author": "FreshWorks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FreshworksStudio/Court-scheduler/issues"
  },
  "homepage": "https://github.com/FreshworksStudio/Court-scheduler#readme",
  "dependencies": {
    "module-alias": "^2.2.2",
    "pipeline-cli": "git+https://github.com/BCDevOps/pipeline-cli.git#v1.1"
  },
  "devDependencies": {
    "@types/node": "^14.10.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  },
  "_moduleAliases": {
    "@app-types": "./dist/src/types/",
    "@lib": "./dist/src/lib/",
    "@config": "./dist/src/config/",
    "@apps": "./dist/src/apps/"
  }
}
